{
  "name": "formufit-client",
  "version": "0.1.0",
  "description": "FormuFit React client is an advanced diet planner app that empowers you to take control of your health and fitness journey",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reshmanidhish/formufit-client.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "dashboard",
    "dashboard-react",
    "formufit-client",
    "formufit-client-react"
  ],
  "author": "FormuFit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reshmanidhish/formufit-client/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.4.0",
    "@fullcalendar/bootstrap": "^5.10.1",
    "@fullcalendar/core": "^5.10.1",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/interaction": "^5.10.1",
    "@fullcalendar/react": "^5.10.1",
    "@stripe/react-stripe-js": "^2.1.2",
    "@stripe/stripe-js": "^2.1.2",
    "axios": "1.4.0",
    "bootstrap": "4.6.2",
    "classnames": "2.3.2",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.2.0",
    "react-dom": "^18.2.0",
    "react-google-login": "^5.2.2",
    "react-rating": "^2.0.5",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.10.0",
    "sass": "1.62.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && echo '/* /index.html 200' | cat >build/_redirects",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/formufit-client.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3"
  }
}
